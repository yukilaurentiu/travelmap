<% if event_owner.avatar.present? %>
<%= image_tag(url_for(event_owner.avatar), class: "avatar dropdown-toggle", id: "navbarDropdown", data: { bs_toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false)  %>
<% else %>
  <%= image_tag "/assets/avataricon.png", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { bs_toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false  %>
<% end %>
